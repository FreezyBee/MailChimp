{
  "name": "freezy-bee/mail-chimp",
  "type": "library",
  "description": "Integration MailChimp 3.0 API into Nette Framework.",
  "keywords": [
    "nette",
    "freezy-bee",
    "MailChimp"
  ],
  "homepage": "http://freezybee.ifire.cz",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Janata",
      "homepage": "https://ifire.cz"
    }
  ],
  "require": {
    "php": "^7.1",
    "nette/application": "^3.0",
    "freezy-bee/httplug": "^3.0",
    "nette/di": "^3.0"
  },
  "require-dev": {
    "nette/bootstrap": "^3.0",
    "php-http/guzzle7-adapter": "^0.1",
    "phpstan/phpstan": "^0.12",
    "squizlabs/php_codesniffer": "^3.4",
    "tracy/tracy": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "FreezyBee\\MailChimp\\": "src/"
    }
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
